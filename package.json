{
    "name": "@illinois-toolkit/ilw-breadcrumbs",
    "description": "Illinois Toolkit: a list of formatted links that provide the path between the siteâ€™s home page and the page the reader is currently viewing.",
    "repository": "github:web-illinois/ilw-breadcrumbs",
    "private": false,
    "license": "MIT",
    "version": "1.2.0",
    "type": "module",
    "files": [
        "src/**",
        "dist/**",
        "builder/**"
    ],
    "exports": {
        ".": {
            "import": "./dist/ilw-breadcrumbs.js",
            "require": "./dist/ilw-breadcrumbs.cjs",
            "default": "./dist/ilw-breadcrumbs.js"
        },
        "./ilw-breadcrumbs.css": {
            "import": "./dist/ilw-breadcrumbs.css"
        },
        "main": "./dist/ilw-breadcrumbs.js",
        "module": "./dist/ilw-breadcrumbs.js"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc --noEmit && vite build --config vite.transpile.config.ts --emptyOutDir && vite build --config vite.build.config.ts --emptyOutDir",
        "preview": "vite preview",
        "prepack": "npm run build"
    },
    "dependencies": {
        "lit": "3.1.3"
    },
    "devDependencies": {
        "typescript": "^5.9.2",
        "vite": "^7.1.3",
        "vite-plugin-dts": "^4.5.4"
    }
}
